{
  "extends": [
    "@cl-tools/eslint-config-cl-react",
    "@cl-tools/eslint-config-cl-typescript"
  ],
  "rules": {
    "no-use-before-define": "off",
    "@typescript-eslint/no-use-before-define": ["error"],
    "@typescript-eslint/no-unused-vars": ["warn"],
    "react/prop-types": "off",
    "no-debugger": "off"
  }
}